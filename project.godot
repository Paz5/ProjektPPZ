; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "LevelData",
"language": "GDScript",
"path": "res://Scripts/Levels/LevelData.gd"
}, {
"base": "Node",
"class": "Manager",
"language": "GDScript",
"path": "res://Scripts/Managers/Manager.gd"
}, {
"base": "Node2D",
"class": "Mob",
"language": "GDScript",
"path": "res://Scripts/Mob.gd"
}, {
"base": "Node",
"class": "MobManager",
"language": "GDScript",
"path": "res://Scripts/Managers/MobManager.gd"
}, {
"base": "Node2D",
"class": "MobTest",
"language": "GDScript",
"path": "res://Scripts/Levels/Test/MobTest.gd"
}, {
"base": "Node",
"class": "TeamManager",
"language": "GDScript",
"path": "res://Scripts/TeamManager.gd"
} ]
_global_script_class_icons={
"LevelData": "",
"Manager": "",
"Mob": "",
"MobManager": "",
"MobTest": "",
"TeamManager": ""
}

[application]

config/name="ProjektPPZ"
run/main_scene="res://Scenes/Root.tscn"
config/icon="res://icon.png"

[autoload]

GameManager="*res://Scripts/Managers/GameManager.gd"
StateManager="*res://Scripts/Managers/StateManager.gd"
PlayerProfileManager="*res://Scripts/Managers/PlayerProfileManager.gd"
LevelManager="*res://Scripts/Managers/LevelManager.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/size/test_width=1920
window/size/test_height=1080
window/stretch/mode="2d"

[editor_plugins]

enabled=PoolStringArray( "res://addons/AsepriteWizard/plugin.cfg" )

[global]

display=false

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
